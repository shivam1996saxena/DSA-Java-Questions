import java.util.Scanner;

public class star_pattern {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int j = 1;
        System.out.println("*");
        for (int i = 0;i<n;i++) {
            if (i-j==0) {
                System.out.print(" ");
            } }
        System.out.println("*");
        j++;

        }
    }
